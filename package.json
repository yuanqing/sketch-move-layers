{
  "private": true,
  "version": "0.0.0",
  "devDependencies": {
    "sketch-plugin-helper": "*"
  },
  "scripts": {
    "build": "sph build",
    "fix": "sph lint --fix",
    "lint": "sph lint",
    "symlink": "sph symlink",
    "unlink": "sph symlink --delete",
    "version": "sph version",
    "watch": "sph build --watch"
  },
  "sketch-plugin-helper": {
    "pluginName": "Move Layers",
    "pluginDescription": "A Sketch plugin for moving layers on the page",
    "authorName": "Lim Yuan Qing",
    "githubUserName": "yuanqing",
    "githubRepositoryName": "sketch-move-layers",
    "menu": [
      {
        "handler": "move-selected-layers",
        "label": "Move Selected Layers"
      },
      "-",
      {
        "handler": "space/space-right",
        "label": "Space Right"
      },
      {
        "handler": "space/space-left",
        "label": "Space Left"
      },
      {
        "handler": "space/space-down",
        "label": "Space Down"
      },
      {
        "handler": "space/space-up",
        "label": "Space Up"
      }
    ],
    "defaults": {
      "moveSelectedLayers.horizontal": "0",
      "moveSelectedLayers.vertical": "0",
      "space": "0"
    }
  }
}
