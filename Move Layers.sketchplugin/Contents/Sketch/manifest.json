{
  "name": "Move Layers",
  "description": "A Sketch plugin for moving the selected layers",
  "author": "Lim Yuan Qing",
  "homepage": "https://github.com/yuanqing/sketch-move-layers",
  "version": "0.0.0",
  "identifier": "yuanqing.sketch-move-layers",
  "appcast": "https://raw.githubusercontent.com/yuanqing/sketch-move-layers/master/appcast.xml",
  "bundleVersion": 1,
  "disableCocoaScriptPreprocessor": true,
  "commands": [
    {
      "name": "Offset Selected Layers",
      "identifier": "yuanqing.sketch-move-layers.offset-selected-layers",
      "script": "plugin.js",
      "handler": "offset-selected-layers"
    },
    {
      "name": "Space Selection Right",
      "identifier": "yuanqing.sketch-move-layers.space-selection-right",
      "script": "plugin.js",
      "handler": "space/space-selection-right"
    },
    {
      "name": "Space Selection Left",
      "identifier": "yuanqing.sketch-move-layers.space-selection-left",
      "script": "plugin.js",
      "handler": "space/space-selection-left"
    },
    {
      "name": "Space Selection Down",
      "identifier": "yuanqing.sketch-move-layers.space-selection-down",
      "script": "plugin.js",
      "handler": "space/space-selection-down"
    },
    {
      "name": "Space Selection Up",
      "identifier": "yuanqing.sketch-move-layers.space-selection-up",
      "script": "plugin.js",
      "handler": "space/space-selection-up"
    }
  ],
  "menu": {
    "title": "Move Layers",
    "items": [
      "yuanqing.sketch-move-layers.offset-selected-layers",
      "-",
      "yuanqing.sketch-move-layers.space-selection-right",
      "yuanqing.sketch-move-layers.space-selection-left",
      "yuanqing.sketch-move-layers.space-selection-down",
      "yuanqing.sketch-move-layers.space-selection-up"
    ]
  }
}
