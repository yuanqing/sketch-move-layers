{
  "name": "Move Layers",
  "description": "A Sketch plugin for moving the selected layers",
  "author": "Lim Yuan Qing",
  "homepage": "https://github.com/yuanqing/sketch-move-layers",
  "version": "0.0.0",
  "identifier": "yuanqing.sketch-move-layers",
  "appcast": "https://raw.githubusercontent.com/yuanqing/sketch-move-layers/master/appcast.xml",
  "bundleVersion": 1,
  "disableCocoaScriptPreprocessor": true,
  "commands": [
    {
      "name": "Offset Selected Layers",
      "identifier": "yuanqing.sketch-move-layers.offset-selected-layers",
      "script": "plugin.js",
      "handler": "offset-selected-layers"
    },
    {
      "name": "Space Selected Layers Right",
      "identifier": "yuanqing.sketch-move-layers.space-selected-layers-right",
      "script": "plugin.js",
      "handler": "space-out-selected-layers/space-selected-layers-right"
    },
    {
      "name": "Space Selected Layers Left",
      "identifier": "yuanqing.sketch-move-layers.space-selected-layers-left",
      "script": "plugin.js",
      "handler": "space-out-selected-layers/space-selected-layers-left"
    },
    {
      "name": "Space Selected Layers Down",
      "identifier": "yuanqing.sketch-move-layers.space-selected-layers-down",
      "script": "plugin.js",
      "handler": "space-out-selected-layers/space-selected-layers-down"
    },
    {
      "name": "Space Selected Layers Up",
      "identifier": "yuanqing.sketch-move-layers.space-selected-layers-up",
      "script": "plugin.js",
      "handler": "space-out-selected-layers/space-selected-layers-up"
    }
  ],
  "menu": {
    "title": "Move Layers",
    "items": [
      "yuanqing.sketch-move-layers.offset-selected-layers",
      "-",
      "yuanqing.sketch-move-layers.space-selected-layers-right",
      "yuanqing.sketch-move-layers.space-selected-layers-left",
      "yuanqing.sketch-move-layers.space-selected-layers-down",
      "yuanqing.sketch-move-layers.space-selected-layers-up"
    ]
  }
}
